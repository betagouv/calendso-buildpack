{
    "name": "Calendso App",
    "description": "Calendso",
    "keywords": [
      "calendso"
    ],
    "env": {
        "NEXTAUTH_SECRET": "secret",
        "NEXTAUTH_URL": {
          "description": "Replace HEROKU_APP_NAME with the name given to your app",
          "value": "https://HEROKU_APP_NAME.herokuapp.com"
        }
    },
    "addons": ["postgresql:postgresql-sandbox"],
    "formation": {
      "web": {
        "amount": 1,
        "size": "S"
      }
    },
    "scripts": {
        "postdeploy": "yarn workspace @calcom/prisma db-deploy"
    }
}

